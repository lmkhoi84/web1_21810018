{{#each data}}
    <h1>{{name}}</h1>
    <div class="mt-3">
        <img src="{{thumbpath}}" alt="{{name}}" class="float-start me-3 rounded-4">
        <div id="about-us-summary">{{{summary}}}</div>
        <a href="#aboutus/{{id}}" onclick="loadDetails('aboutus/{{id}}','details-template','content')" class="btn btn-link readmore ps-0">Read more</a>
    </div>
{{/each}}